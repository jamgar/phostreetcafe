---
import { Icon } from "astro-icon";
import { Image } from "astro:assets";
import BrandLogo from "../assets/images/psc-logo.png";
---

<div id="navbar-desktop" class="desktop-menu">
  <div class="brand">
    <a href="#hero" class="brand-logo">
      <Image src={BrandLogo} alt="Pho Street Cafe Logo" width={50} />
    </a>
    <p>PHO STREET CAFE</p>
  </div>
  <nav>
    <a href="#menu">Menu</a>
    <a href="#contact">Contact</a>
    <a href="#location">Location</a>
  </nav>
  <div id="ordernow">
    <span class="ordernow-label">Order Now:</span>
    <div class="nav-btn-group">
      <a href="#" class="btn-light nav-btn-call">
        <Icon class="icon" name="call" />
        <span>(972) 429-7212</span>
      </a>
      <a
        href="https://www.ubereats.com/store/pho-street-cafe/E0prjAv6Qhy5GndiOh1Prg"
        class="btn-dark btn-uber"
      >
        <span>Uber</span>
        <span>Eats</span>
      </a>
      <a href="https://go.lavutogo.com/phostreetcafe" class="btn-dark btn-lavu">
        <svg
          width="59"
          height="18"
          viewBox="0 0 59 21"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink"
        >
          <rect width="59" height="21" fill="url(#pattern0)"></rect>
          <defs>
            <pattern
              id="pattern0"
              patternContentUnits="objectBoundingBox"
              width="1"
              height="1"
            >
              <use
                xlink:href="#image0_695_3990"
                transform="matrix(0.00116318 0 0 0.00326797 -0.00947159 0)"
              ></use>
            </pattern>
            <image
              id="image0_695_3990"
              width="876"
              height="306"
              xlink:href="data:image/png;base64,"
            />
          </defs>
        </svg>
      </a>
    </div>
  </div>
</div>

<style>
  .desktop-menu {
    display: none;
  }
  .brand {
    color: var(--base-light);
    display: flex;
    align-items: center;
    font-weight: 600;
  }
  .brand-logo {
    margin-right: 4px;
  }
  nav a,
  #ordernow a {
    text-decoration: none;
  }
  nav a {
    color: var(--base-dark);
    padding: 0 20px;
  }
  .ordernow-label {
    font-size: 12px;
    font-weight: 600;
    margin: 0 5px;
  }
  .nav-btn-group {
    display: flex;
  }
  .nav-btn-group a {
    padding: 4px 14px;
    margin: 0 5px;
    border-radius: 4px;
    text-align: center;
  }
  .nav-btn-call {
    align-items: center;
    justify-content: center;
    display: flex;
    font-size: 12px;
  }
  .icon {
    height: 14px;
    display: inline-block;
  }
  .btn-lavu {
    display: flex;
    align-items: center;
  }
  .btn-uber span {
    font-size: 18px;
  }
  .btn-uber span:first-child {
    font-weight: 400;
  }
  .btn-uber span:last-child {
    font-weight: 700;
    margin-top: -20px;
  }
  .sticky {
    position: sticky;
    top: 0;
    width: 100%;
    z-index: 100;
  }

  @media only screen and (min-width: 768px) {
    .desktop-menu {
      background-color: var(--base-light);
      display: flex;
      justify-content: space-between;
      align-items: flex-end;
      padding-left: 25px;
      padding-bottom: 8px;
    }
    .desktop-menu nav a:hover {
      color: #3a3a3a;
    }
    .brand {
      color: var(--base-dark);
      font-size: 14px;
    }
  }
  @media only screen and (min-width: 1024px) {
    .brand {
      font-size: 20px;
    }
  }
</style>

<script>
  import "../scripts/scroll";
</script>
